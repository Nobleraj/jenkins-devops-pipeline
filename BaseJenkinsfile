def imageTag = 'latest'
def imageVersion = '1.0'

pipeline {
    agent any

    stages {
        stage('Build base image') {
            steps {
                script {
                    // Build the Docker image
                   docker.build('baseimage:${imageTag}:${imageVersion}', '-f BaseDockerfile .')
                }
            }
        }
    }
    
}
